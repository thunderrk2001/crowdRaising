<body>
    <div class="container">

        <header class="d-flex flex-wrap justify-content-center py-3 mb-4 border-bottom">
            <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-dark text-decoration-none">
                <svg class="bi me-2" width="40" height="32"><use xlink:href="#bootstrap"></use></svg>
                <span class="fs-2" style="font-weight: bolder;">Crowd Raising</span>
            </a>

            <ul class="nav nav-pills">
                <li class="nav-item"><a href="/" class="nav-link active" style="background-color: #385277;" aria-current="page">Home</a></li>
                <li class="nav-item"><button style="color: #385277;" class="nav-link" data-mdb-toggle="modal" data-mdb-target="#staticBackdrop">Create Fund</button></li>
                <li class="nav-item"> <span class="nav-link " id="myAccount" aria-current="page">Not connected</span></li>
            </ul>
        </header>
    </div>
    <main class="container" id="container">
        <div class="d-flex align-items-center p-3 my-3 text-white  rounded shadow-sm" style="background-color: rgb(123, 124, 123);">
            <div class="lh-1">
                <h1 class="h4 mb-0 text-white lh-1">Open Funds</h1>

            </div>
        </div>


    </main>

    <div class="modal fade" id="staticBackdrop" data-mdb-backdrop="static" data-mdb-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog">

            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="staticBackdropLabel">Create Fund</h5>
                    <button type="button" class="btn-close" data-mdb-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                </div>
                <div id="errorMessage" style="text-align: center; color: Red;"></div>
                <main class="container">
                    <form class="row g-3 needs-validation" novalidate>
                        <div class="col-md-4">
                            <label for="fundName" class="form-label">Fund Name</label>
                            <input type="text" class="form-control" id="fundName" value="" required>
                            <div class="valid-feedback">
                                Looks good!
                            </div>
                        </div>
                        <div class="col-md-4">
                            <label for="minAmount" class="form-label">Min Amount(eth)</label>
                            <input type="number" step="0.000000000000000001" class="form-control" id="minAmount" value="" required>
                            <div class="valid-feedback">
                                Looks good!
                            </div>
                        </div>
                        <div class="mb-3">
                            <label for="fundDesc" class="form-label">Fund Description</label>
                            <textarea class="form-control is-invalid" id="fundDesc" placeholder="For which community" required></textarea>

                        </div>

                        <div class="col-12">
                            <button class="btn btn-primary mb-3" style="overflow: hidden;
            width: 130px;
            height: 40px;">Create Fund</button>
                        </div>
                    </form>
                </main>


            </div>
        </div>

    </div>

    </div>
